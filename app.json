{
   "expo": {
      "name": "DiceNote",
      "scheme": "dicenote",
      "slug": "tweezer",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "dark",
      "splash": {
         "backgroundColor": "#08101B"
      },
      "ios": {
         "supportsTablet": true,
         "userInterfaceStyle": "dark"
      },
      "android": {
         "package": "com.hhpaek.dicenote",
         "backgroundColor": "#000710",
         "userInterfaceStyle": "dark"
      },
      "web": {
         "favicon": "./assets/favicon.png"
      },
      "plugins": [
         "expo-router",
         [
            "expo-image-picker",
            {
               "photosPermission": "The app can access your photos to add images to your notes."
            }
         ],
         [
            "expo-asset",
            {
               "assets": [
                  "./assets/icon.png"
               ]
            }
         ],
         [
            "expo-local-authentication",
            {
               "faceIDPermission": "Allow DiceNote to use Face ID."
            }
         ]
      ],
      "extra": {
         "eas": {
            "projectId": "1d5e3e29-18ac-444b-9064-eadb31cda2f3"
         }
      }
   }
}